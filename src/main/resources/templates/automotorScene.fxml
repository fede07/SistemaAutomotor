<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="473.0" prefWidth="815.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="grupo2.SistemaAutomotor.controlador.AutomotorControlador">
    <BorderPane prefWidth="1000.0">
        <left>
            <fx:include source="mainScene.fxml" />
        </left>
        <center>
            <AnchorPane layoutX="41.0" layoutY="100.0" prefHeight="750.0" prefWidth="100.0">
                <Label layoutX="173.0" layoutY="93.0" text="Dominio">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Label>
                <Label layoutX="403.0" layoutY="93.0" text="DNI Titular">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Label>
                <Label layoutX="176.0" layoutY="139.0" text="Modelo">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Label>
                <Label layoutX="405.0" layoutY="139.0" prefHeight="17.0" prefWidth="42.0" text="Marca">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Label>
                <Label layoutX="177.0" layoutY="185.0" text="Año">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Label>
                <Label layoutX="404.0" layoutY="185.0" text="Municipio">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Label>
                <TextField fx:id="dominioAutomotorTexto" layoutX="173.0" layoutY="110.0" prefHeight="25.0" prefWidth="202.0" />
                <TextField fx:id="titularAutomotorTexto" layoutX="403.0" layoutY="110.0" prefHeight="25.0" prefWidth="202.0" />
                <TextField fx:id="modeloAutomotorTexto" layoutX="174.0" layoutY="156.0" prefHeight="25.0" prefWidth="202.0" />
                <TextField fx:id="marcaAutomotorTexto" layoutX="403.0" layoutY="156.0" prefHeight="25.0" prefWidth="202.0" />
                <TextField fx:id="anioAutomotorTexto" layoutX="175.0" layoutY="202.0" prefHeight="25.0" prefWidth="202.0" />
                <Button fx:id="agregarBoton" layoutX="302.0" layoutY="245.0" mnemonicParsing="false" onAction="#agregarAutomotor" style="-fx-background-color: #6161dc;" text="Agregar" textFill="WHITE" />
                <Button fx:id="modificarBoton" layoutX="368.0" layoutY="245.0" mnemonicParsing="false" onAction="#modificarAutomotor" style="-fx-background-color: #6161dc;" text="Modificar" textFill="WHITE" />
                <Button fx:id="EliminarBoton" layoutX="443.0" layoutY="245.0" mnemonicParsing="false" onAction="#eliminarAutomotor" style="-fx-background-color: #ff4545;" text="Eliminar" textFill="WHITE" />
                <Button fx:id="limpiarBoton" layoutX="302.0" layoutY="282.0" mnemonicParsing="false" onAction="#limpiarFormulario" prefHeight="25.0" prefWidth="202.0" style="-fx-background-color: #ffc44a;" text="Limpiar Formulario" />
                <ComboBox fx:id="municipioComboBox" layoutX="403.0" layoutY="202.0" prefHeight="25.0" prefWidth="202.0" />
               <TableView fx:id="automotorTabla" layoutX="130.0" layoutY="343.0" onMouseClicked="#cargarAutomotorFormulario" prefHeight="369.0" prefWidth="540.0">
                   <columns>
                       <TableColumn fx:id="dominioAutomotorColumna" prefWidth="75.0" text="Dominio" />
                       <TableColumn fx:id="titularAutomotorColumna" prefWidth="75.0" text="DNI Titular" />
                       <TableColumn fx:id="modeloAutomotorColumna" prefWidth="75.0" text="Modelo" />
                       <TableColumn fx:id="marcaAutomotorColumna" prefWidth="75.0" text="Marca" />
                       <TableColumn fx:id="anioAutomotorColumna" prefWidth="75.0" text="Año" />
                       <TableColumn fx:id="municipioAutomotorColumna" prefWidth="75.0" text="Municipio" />
                   </columns>
               </TableView>
            </AnchorPane>
        </center>
    </BorderPane>
</AnchorPane>
