<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="grupo2.SistemaAutomotor.controlador.GestionarControlador">
    <left>
        <fx:include source="mainScene.fxml" BorderPane.alignment="TOP_LEFT" />
    </left>
    <center>
        <VBox alignment="TOP_CENTER" prefHeight="1080.0" stylesheets="@../styles/content.css" BorderPane.alignment="CENTER">
            <VBox alignment="TOP_CENTER" maxHeight="125.0" minHeight="125.0" styleClass="title_container">
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Gestionar" />
            </VBox>
             <TabPane prefWidth="800.0" snapToPixel="false" styleClass="menu_container" tabClosingPolicy="UNAVAILABLE" tabMaxHeight="500.0" tabMaxWidth="500.0" tabMinWidth="245.0">
                 <opaqueInsets>
                     <Insets />
                 </opaqueInsets>
                 <Tab text="Agregar">
                     <fx:include source="agregarScene.fxml" />
                 </Tab>
                 <Tab text="Modificar">
                     <fx:include source="modificarScene.fxml" />
                 </Tab>
                 <Tab text="Eliminar">
                     <fx:include source="eliminarScene.fxml" />
                 </Tab>
                 <Tab text="Consultar">
                     <fx:include source="consultarScene.fxml" />
                 </Tab>
             </TabPane>
        </VBox>
    </center>
</BorderPane>
